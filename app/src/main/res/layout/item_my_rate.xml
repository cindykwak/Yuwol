    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="분위기 "
        android:fontFamily="@font/suit"
        android:textColor="@color/gray_100"
        android:textSize="8sp"
        app:layout_constraintTop_toTopOf="@+id/tv_my_rate_difficulty"
        app:layout_constraintStart_toEndOf="@id/tv_my_rate_difficulty" />

    <TextView
        android:id="@+id/tv_my_rate_mood"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="보통"
        android:fontFamily="@font/suit"
        android:textColor="@color/pink_100"
        android:textSize="8sp"
        app:layout_constraintTop_toTopOf="@+id/textView1"
        app:layout_constraintStart_toEndOf="@+id/textView1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="고음 ★"
        android:fontFamily="@font/suit"
        android:textColor="@color/gray_100"
        android:textSize="8sp"
        app:layout_constraintStart_toStartOf="@id/textView"
        app:layout_constraintTop_toBottomOf="@+id/tv_my_rate_mood" />

    <TextView
        android:id="@+id/tv_my_rate_high"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:fontFamily="@font/suit"
        android:textSize="8sp"
        android:textColor="@color/gray_100"
        app:layout_constraintBaseline_toBaselineOf="@id/textView2"
        app:layout_constraintStart_toEndOf="@id/textView2"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="4dp"
        android:text="|"
        android:fontFamily="@font/suit"
        android:textSize="8sp"
        android:textColor="@color/gray_100"
        app:layout_constraintTop_toTopOf="@id/textView2"
        app:layout_constraintStart_toEndOf="@id/tv_my_rate_high"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="저음 ★"
        android:fontFamily="@font/suit"
        android:textSize="8sp"
        android:textColor="@color/gray_100"
        app:layout_constraintTop_toTopOf="@id/textView2"
        app:layout_constraintStart_toEndOf="@id/textView5"/>

    <TextView
        android:id="@+id/tv_my_rate_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:fontFamily="@font/suit"
        android:textSize="8sp"
        android:textColor="@color/gray_100"
        app:layout_constraintBaseline_toBaselineOf="@id/textView2"
        app:layout_constraintStart_toEndOf="@id/textView3"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="4dp"
        android:text="|"
        android:textSize="8sp"
        android:fontFamily="@font/suit"
        android:textColor="@color/gray_100"
        app:layout_constraintTop_toTopOf="@id/textView2"
        app:layout_constraintStart_toEndOf="@id/tv_my_rate_low"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="랩 ★"
        android:fontFamily="@font/suit"
        android:textColor="@color/gray_100"
        android:textSize="8sp"
        app:layout_constraintStart_toEndOf="@id/textView6"
        app:layout_constraintTop_toTopOf="@id/textView2" />

    <TextView
        android:id="@+id/tv_my_rate_rap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:fontFamily="@font/suit"
        android:textSize="8sp"
        android:textColor="@color/gray_100"
        app:layout_constraintBaseline_toBaselineOf="@id/textView2"
        app:layout_constraintStart_toEndOf="@id/textView4"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/ib_my_rate_delete"
        android:layout_width="40sp"
        android:layout_height="20sp"
        android:layout_marginTop="6dp"
        android:background="@drawable/bg_rectangle_shadow"
        android:text="삭제"
        android:fontFamily="@font/suit"
        android:textColor="@color/gray_100"
        android:textSize="8sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/tv_my_rate_date"
        android:layout_width="43dp"
        android:layout_height="12dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="22.12.25"
        android:fontFamily="@font/suit"
        android:textSize="8sp"
        android:textColor="@color/gray_400"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@id/ib_my_rate_delete" />


</androidx.constraintlayout.widget.ConstraintLayout>