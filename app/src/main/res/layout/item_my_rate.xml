<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginStart="12dp">

    <androidx.cardview.widget.CardView
        android:id="@+id/cv_my_rate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent">
        <ImageView
            android:id="@+id/iv_my_rate_cover"
            android:layout_width="95dp"
            android:layout_height="95dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:srcCompat="@tools:sample/avatars"/>
    </androidx.cardview.widget.CardView>


    <TextView
        android:id="@+id/tv_my_rate_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Title"
        android:fontFamily="@font/suit"
        android:textSize="12sp"
        android:maxLines="1"
        android:ellipsize="end"
        app:layout_constraintTop_toBottomOf="@id/cv_my_rate"
        app:layout_constraintStart_toStartOf="@id/cv_my_rate"/>

    <TextView
        android:id="@+id/tv_my_rate_artist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="Artist"
        android:fontFamily="@font/suit"
        android:textSize="10sp"
        android:textColor="@color/gray_300"
        android:maxLines="1"
        android:ellipsize="end"
        app:layout_constraintTop_toBottomOf="@id/tv_my_rate_title"
        app:layout_constraintStart_toStartOf="@id/tv_my_rate_title"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="가창 난이도 ★"
        android:fontFamily="@font/suit"
        android:textSize="8sp"
        android:textColor="@color/pink_100"
        app:layout_constraintTop_toBottomOf="@id/tv_my_rate_artist"
        app:layout_constraintStart_toStartOf="@id/tv_my_rate_artist"/>

    <TextView
        android:id="@+id/tv_my_rate_difficulty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:fontFamily="@font/suit"
        android:textSize="8sp"
        android:textColor="@color/pink_100"
        app:layout_constraintTop_toTopOf="@id/textView"
        app:layout_constraintStart_toEndOf="@id/textView"/>

