<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="2dp">

    <androidx.cardview.widget.CardView
        android:id="@+id/cv_my_rate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="3dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent">
        <ImageView
            android:id="@+id/iv_my_rate_cover"
            android:layout_width="120dp"
            android:layout_height="120dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:srcCompat="@tools:sample/avatars"/>
    </androidx.cardview.widget.CardView>


    <TextView
        android:id="@+id/tv_my_rate_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Title"
        android:textSize="13sp"
        app:layout_constraintTop_toBottomOf="@id/cv_my_rate"
        app:layout_constraintStart_toStartOf="@id/cv_my_rate"/>

    <TextView
        android:id="@+id/tv_my_rate_artist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Artist"
        android:textSize="11sp"
        android:textColor="@color/gray_100"
        app:layout_constraintTop_toBottomOf="@id/tv_my_rate_title"
        app:layout_constraintStart_toStartOf="@id/tv_my_rate_title"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="가창 난이도 ★"
        android:textSize="11sp"
        android:textColor="@color/pink_100"
        app:layout_constraintTop_toBottomOf="@id/tv_my_rate_artist"
        app:layout_constraintStart_toStartOf="@id/tv_my_rate_artist"/>

    <TextView
        android:id="@+id/tv_my_rate_difficulty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="11sp"
        android:textColor="@color/pink_100"
        app:layout_constraintTop_toTopOf="@id/textView"
        app:layout_constraintStart_toEndOf="@id/textView"/>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="분위기 "
        android:textColor="@color/gray_100"
        android:textSize="11sp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintStart_toEndOf="@+id/tv_my_rate_difficulty" />

    <TextView
        android:id="@+id/tv_my_rate_mood"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="보통"
        android:textColor="@color/pink_100"
        android:textSize="11sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_my_rate_rap"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="고음 ★"
        android:textColor="@color/gray_100"
        android:textSize="9sp"
        app:layout_constraintStart_toStartOf="@id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/tv_my_rate_high"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="9sp"
        android:textColor="@color/gray_100"
        app:layout_constraintTop_toTopOf="@id/textView2"
        app:layout_constraintStart_toEndOf="@id/textView2"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="4dp"
        android:text="|"
        android:textSize="9sp"
        android:textColor="@color/gray_100"
        app:layout_constraintTop_toTopOf="@id/textView2"
        app:layout_constraintStart_toEndOf="@id/tv_my_rate_high"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="저음 ★"
        android:textSize="9sp"
        android:textColor="@color/gray_100"
        app:layout_constraintTop_toTopOf="@id/textView2"
        app:layout_constraintStart_toEndOf="@id/textView5"/>

    <TextView
        android:id="@+id/tv_my_rate_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="9sp"
        android:textColor="@color/gray_100"
        app:layout_constraintTop_toTopOf="@id/textView2"
        app:layout_constraintStart_toEndOf="@id/textView3"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="4dp"
        android:text="|"
        android:textSize="9sp"
        android:textColor="@color/gray_100"
        app:layout_constraintTop_toTopOf="@id/textView2"
        app:layout_constraintStart_toEndOf="@id/tv_my_rate_low"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="랩 ★"
        android:textColor="@color/gray_100"
        android:textSize="9sp"
        app:layout_constraintStart_toEndOf="@id/textView6"
        app:layout_constraintTop_toTopOf="@id/textView2" />

    <TextView
        android:id="@+id/tv_my_rate_rap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="9sp"
        android:textColor="@color/gray_100"
        app:layout_constraintTop_toTopOf="@id/textView2"
        app:layout_constraintStart_toEndOf="@id/textView4"/>

    <Button
        android:id="@+id/ib_my_rate_delete"
        android:layout_width="50sp"
        android:layout_height="30sp"
        android:backgroundTint="#313131"
        android:text="삭제"
        android:textSize="8sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/tv_my_rate_date"
        android:layout_width="43dp"
        android:layout_height="12dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="22.12.25"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_my_rate_rap" />


</androidx.constraintlayout.widget.ConstraintLayout>