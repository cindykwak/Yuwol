<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/iv_item_chart_all_cover"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@tools:sample/avatars"/>

    <TextView
        android:id="@+id/tv_item_chart_all_rank"
        android:layout_width="30dp"
        android:layout_height="60dp"
        android:text="1"
        android:textSize="18sp"
        android:textStyle="bold"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@id/iv_item_chart_all_cover"
        app:layout_constraintStart_toEndOf="@id/iv_item_chart_all_cover"/>

        <TextView
            android:id="@+id/tv_item_chart_all_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Title"
            android:textSize="11sp"
            app:layout_constraintStart_toEndOf="@+id/tv_item_chart_all_rank"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_item_chart_all_artist"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Artist"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@+id/tv_item_chart_all_rank"
            app:layout_constraintTop_toBottomOf="@+id/tv_item_chart_all_title" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="가창 난이도 ★"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@id/tv_item_chart_all_rank"
            app:layout_constraintTop_toBottomOf="@id/tv_item_chart_all_artist" />

        <TextView
            android:id="@+id/tv_item_chart_all_difficulty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@id/textView7"
            app:layout_constraintTop_toTopOf="@id/textView7" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="분위기 "
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@+id/tv_item_chart_all_difficulty"
            app:layout_constraintTop_toTopOf="@+id/tv_item_chart_all_difficulty" />

        <TextView
            android:id="@+id/tv_item_chart_all_mood"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="보통"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@+id/textView"
            app:layout_constraintTop_toTopOf="@+id/textView" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="고음 ★"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@id/tv_item_chart_all_rank"
            app:layout_constraintTop_toBottomOf="@id/tv_item_chart_all_mood" />

        <TextView
            android:id="@+id/tv_item_chart_all_high"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@id/textView2"
            app:layout_constraintTop_toTopOf="@id/textView2" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="4dp"
            android:text="|"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@id/tv_item_chart_all_high"
            app:layout_constraintTop_toTopOf="@id/tv_item_chart_all_high" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:text="저음 ★"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@id/textView5"
            app:layout_constraintTop_toTopOf="@id/textView5" />

        <TextView
            android:id="@+id/tv_item_chart_all_low"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@id/textView3"
            app:layout_constraintTop_toTopOf="@id/textView3" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="4dp"
            android:text="|"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@id/tv_item_chart_all_low"
            app:layout_constraintTop_toTopOf="@id/tv_item_chart_all_low" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:text="랩 ★"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@id/textView6"
            app:layout_constraintTop_toTopOf="@id/textView6" />

        <TextView
            android:id="@+id/tv_item_chart_all_rap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@id/textView4"
            app:layout_constraintTop_toTopOf="@id/textView4" />

</androidx.constraintlayout.widget.ConstraintLayout>